  "license": "MIT OR Apache-2.0",
  "bugs": {
    "url": "https://github.com/yvvki/revolt-rs/issues"
  },
  "homepage": "https://github.com/yvvki/revolt-rs#readme",
  "devDependencies": {
    "@ltd/j-toml": "^1.35.2",
    "@openapitools/openapi-generator-cli": "^2.5.1"
  }
}
